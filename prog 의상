import java.util.*;
import java.io.*;
class Solution {
    public int solution(String[][] clothes) {
        Map<String, Integer> map = new HashMap<>();
        for(int i =0;i<clothes.length;i++){
            map.put(clothes[i][1],map.getOrDefault(clothes[i][1],0)+1); //주의!! getOrDefault할 때 get할 값을 앞에 적어줘야 함!!여기서는 String임. get 역할을 하는 함수의 일종임.
        }

        int total =1;
        for(String key: map.keySet()){
            total *= (map.get(key)+1);
        }
        return total-1;
    }
}
